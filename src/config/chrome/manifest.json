{
  "name": "Otree4Gitlab",
  "version": "0.0.6",
  "manifest_version": 2,
  "author": "Louis Zhang",
  "description": "explorer tree for dated GitLab(forked from octotree)",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "optional_permissions": ["<all_urls>"],
  "options_ui": {
    "page": "static/options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  }
}
